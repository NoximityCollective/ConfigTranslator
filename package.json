{
  "name": "configtranslator",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:cf": "wrangler pages dev --config wrangler.local.toml",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "deploy": "./scripts/deploy-local.sh",
    "deploy:manual": "npm run build && cp wrangler.local.toml wrangler.toml && wrangler pages deploy .next --project-name configtranslator && git checkout wrangler.toml",
    "setup:db": "wrangler d1 execute configtranslator-db --file=./schema.sql --remote",
    "setup:local-db": "wrangler d1 execute configtranslator-db --file=./schema.sql",
    "setup:secrets": "echo 'Run: wrangler secret put OPENROUTER_API_KEY' && echo 'Run: wrangler secret put IP_HASH_SALT'",
    "db:query": "wrangler d1 execute configtranslator-db --remote",
    "kv:list": "wrangler kv key list",
    "logs": "wrangler pages deployment tail"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.511.0",
    "next": "15.3.2",
    "next-themes": "^0.4.6",
    "openai": "^4.103.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.3.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.0",
    "typescript": "^5",
    "wrangler": "^3.0.0"
  }
}